            function removeIfInvalid(event, ui, _this, ul) {
                _this = $(_this);
                // Selected an item, nothing to do
                if (ui.item) {
                    return;
                } else {
                    _this.prop('title', 'Selecciona ' + _this.attr('aria-label'));
                    _this.tooltip({
                        my: 'bottom', at: "left-50 bottom", collision: "flip",
                        show: {effect: "fade", duration: 800},
                        hide: {effect: "fade", duration: 800},
                        classes: {
                            "ui-tooltip": "ui-state-highlight"
                        }
                    });
                    _this.tooltip('open');
                    setTimeout(function () {
                        _this.tooltip('close');
                        _this.prop('title', '');
                    }, 3000);
                    _this.val("");
                    deshabilitar(_this);
                }

                // Search for a match (case-insensitive)
                var value = _this.val(),
                        valueLowerCase = value.toLowerCase(),
                        valid = false;

                $('#' + ul.id).children("li").each(function () {
                    if ($(this).children('div').text().toLowerCase() === valueLowerCase) {
                        valid = true;
                        return false;
                    }
                });
                // Found a match, nothing to do
                if (valid) {
                    return;
                }

                // Remove invalid value
                _this.val("");

                _this.autocomplete("instance").term = "";
            }

