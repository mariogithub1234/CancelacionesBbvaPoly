            function valida(item) {
                var valores = '';
                var chk = item.attr('id') === 'pantalla_3' ? true : false;

                item.find("input[type='text']").each(function () {
                    if (chk) {
                        if ($('#chk_credito_comp').is(':checked') && !$('#chk_credito_cont').is(':checked') && !$('#chk_credito_cancel').is(':checked')) {
                            if ($(this).val() === '') {
                                valores += $(this).attr('aria-label') + '<br>';
                            }
                        } else if (!$('#chk_credito_comp').is(':checked') && !$('#chk_credito_cont').is(':checked') && !$('#chk_credito_cancel').is(':checked')) {
                            if ($(this).attr('id') !== 'int_fin_2') {
                                if ($(this).val() === '') {
                                    valores += $(this).attr('aria-label') + '<br>';
                                }
                            }
                        }

                    } else {
                        if ($(this).attr('id') !== 'valor_4' && $(this).attr('id') !== 'valor_5') {
                            if ($(this).val() === '') {
                                valores += $(this).attr('aria-label') + '<br>';
                            }
                        }
                    }

                });
                if (valores !== '') {
                    $('#item_valida').html('').html(valores);
                    $('#modal_valida').modal('show');
                    return false;
                } else {
                    return true;
                }


            }

